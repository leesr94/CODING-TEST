// 00:25:13

import java.util.*;

class Solution {
    public List<Long> solution(int x, int n) {
        List<Long> answer = new ArrayList<Long>();
        
        if (x >= -10000000 && x <= 10000000 && n <= 1000) {
            for (long i = 1; i <= n; i++) {
                answer.add(x * i);
            }
        }
        
        return answer;
    }
}
